// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    nome String
    email String @unique
    senha String

    @@map("users")
}

model Dados {
  id Int @id @default(autoincrement())
  sobrenome String
  idade Int
  endereco String
  data_nascimento String
  pai String
  mae String
  genero String

  @@map("dados")
}